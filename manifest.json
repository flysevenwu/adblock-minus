{
  "name": "Adblock Minus",
  "version": "{{&version}}",
  "manifest_version": 2,
  "minimum_chrome_version": "47",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "webNavigation",
    "storage"
    ],
    "background": {
      "scripts": [
        "bundle.js"
        ]
    },
    "homepage_url": "https://github.com/{{&repository}}"
}
